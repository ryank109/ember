<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>User Name</th>
            <th>First Name</th>
            <th>Last Name</th>
        </tr>
    </thead>
    <tbody>
        {{#each model}}
            <tr {{action 'showDetail' this}}>
                <td>{{id}}</td>
                <td>{{username}}</td>
                <td>{{firstName}}</td>
                <td>{{lastName}}</td>
            </tr>
        {{/each}}
    </tbody>
</table>

<div class="container-fluid">
    <div class="row" style="padding-top:50px;">
        <div class="col-xs-12">
            {{outlet}}
        </div>
    </div>
</div>
